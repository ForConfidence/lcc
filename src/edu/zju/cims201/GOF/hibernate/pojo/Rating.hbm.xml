<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
        <!-- edu.zju.cims201.GOF.hibernate.pojo.Rating root -->
        <class name="edu.zju.cims201.GOF.hibernate.pojo.Rating" table="caltks.Rating">
                <id name="id" type="java.lang.Long" column="id">
                        <generator class="increment"/>
                </id>
                
                <many-to-one name="rater" column="raterID" not-null="true" 
                class="edu.zju.cims201.GOF.hibernate.pojo.SystemUser">
                </many-to-one>
                <property name="ratingTime" column="ratingTime" type="timestamp"/>
                <property name="score" column="score" type="float"/>
                
                <many-to-one name="knowledge" column="knowledgeID" not-null="true" 
                class="edu.zju.cims201.GOF.hibernate.pojo.MetaKnowledge">
                </many-to-one>
               
        </class>
</hibernate-mapping>