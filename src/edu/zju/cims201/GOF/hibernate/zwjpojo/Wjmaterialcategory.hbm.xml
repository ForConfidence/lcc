<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="edu.zju.cims201.GOF.hibernate.zwjpojo.Wjmaterialcategory" table="wjmaterialcategory">
		<id name="id" type="int" column="id" >
			<generator class="increment" />
		</id>
		<property name="categoryname" column="categoryname" type="string" length="250"  />
		<property name="englishname" column="englishname" type="string" length="250" />
		<property name="categoryintro" column="categoryintro" type="string" length="250" />
		<property name="UUID" column="UUID" type="string" length="250" />
		<property name="parentid" column="parentid" type="int"/>
	</class>
</hibernate-mapping>