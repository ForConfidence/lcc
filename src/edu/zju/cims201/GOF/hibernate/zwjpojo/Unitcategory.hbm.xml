<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="edu.zju.cims201.GOF.hibernate.zwjpojo.Unitcategory" table="Unitcategory">
		<id name="id" type="int" column="id" >
			<generator class="increment" />
		</id>
		<property name="unitcategoryname" column="unitcategoryname" type="string" length="250"  />
		<property name="unitcategoryintro" column="unitcategoryintro" type="string" length="250" />
		<property name="defaultflowproperty" column="defaultflowproperty" type="string" length="250" />
		<property name="parentname" column="parentname" type="string" length="250" />
		<property name="uuid" column="uuid" type="string" length="250" />
		<property name="parentid" column="parentid" type="int"/>
	</class>
</hibernate-mapping>