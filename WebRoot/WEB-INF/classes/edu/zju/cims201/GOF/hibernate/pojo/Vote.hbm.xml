<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
        <!-- edu.zju.cims201.GOF.hibernate.pojo.Vote root -->
        <class name="edu.zju.cims201.GOF.hibernate.pojo.Vote" table="caltks.Vote">
                <id name="id" type="java.lang.Long" column="id">
                        <generator class="increment"/>
                </id>
                 <many-to-one name="user" column="userID"
                class="edu.zju.cims201.GOF.hibernate.pojo.SystemUser"
                not-null="true">
                </many-to-one>
                <property name="isSupport" column="isSupport" type="boolean"/>
                <property name="voteTime" column="voteTime" type="timestamp"/>
                 <many-to-one name="comment" column="commentID"
                class="edu.zju.cims201.GOF.hibernate.pojo.Comment"
                not-null="true">
                </many-to-one>
                
        </class>
</hibernate-mapping>